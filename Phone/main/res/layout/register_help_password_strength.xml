<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	style="@style/WhiteTheme">
	
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		
		<View
			android:id="@+id/account_info_two_uid_bar_one_help"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignTop="@id/account_info_help_not_valid_label"
			android:layout_marginLeft="32dp"
			android:layout_marginTop="8dp"
			android:layout_toRightOf="@id/account_info_help_not_valid_label"
			android:background="@color/red" />
		
		<View
			android:id="@+id/account_info_two_uid_bar_two_help"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignTop="@id/account_info_two_uid_bar_one_help"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@id/account_info_two_uid_bar_one_help"
			android:background="@android:color/darker_gray" />
		
		<View
			android:id="@+id/account_info_two_uid_bar_three_help"
			android:layout_alignTop="@id/account_info_two_uid_bar_one_help"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@id/account_info_two_uid_bar_two_help"
			android:background="@android:color/darker_gray" />
		
		<TextView
			android:id="@+id/account_info_help_title_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_alignParentLeft="true"
			android:layout_alignParentTop="true"
			android:text="@string/account_info_help_pass_strength_text"
			android:textAppearance="?android:attr/textAppearanceMedium"
			android:textColor="@color/black"
			android:textStyle="bold" />
			
		<View
			android:id="@+id/account_info_help_moderate_bar_one"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignLeft="@id/account_info_two_uid_bar_one_help"
			android:layout_below="@id/account_info_help_separator_line_one"
			android:layout_alignTop="@id/account_info_help_p2_title_label"
			android:layout_marginTop="8dp"
			android:background="@color/yellow" />
		
		<View
			android:id="@+id/account_info_help_moderate_bar_two"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignTop="@id/account_info_help_moderate_bar_one"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@id/account_info_help_moderate_bar_one"
			android:background="@color/yellow" />
		
		<View
			android:id="@+id/account_info_help_moderate_bar_three"
			android:layout_alignTop="@id/account_info_help_moderate_bar_one"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@id/account_info_help_moderate_bar_two"
			android:background="@android:color/darker_gray" />
		
		<TextView
			android:id="@+id/account_info_help_not_valid_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:layout_below="@id/account_info_help_title_label"
			android:paddingTop="8dp"
			android:text="@string/account_info_help_pass_strength_text"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		
		<TextView
			android:id="@+id/account_info_two_uid_strength_bar_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="4dp"
			android:layout_below="@+id/account_info_two_uid_bar_one_help"
			android:layout_alignLeft="@+id/account_info_two_uid_bar_one_help"
			android:text="@string/strength_bar_not_valid"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall"/>
		
		<View
			android:id="@+id/account_info_help_separator_line_one"
			android:layout_width="fill_parent"
			android:layout_height="1dp"
			android:layout_marginTop="16dp"
			android:layout_below="@+id/account_info_help_not_valid_detail_label"
			android:background="@android:color/darker_gray" />
		
		<TextView
			android:id="@+id/account_info_help_not_valid_detail_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/account_info_help_not_valid_label"
			android:layout_marginTop="16dp"
			android:text="@string/account_info_help_pass_p1_text"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:textColor="@color/black"
			android:textSize="@dimen/light_text_size" />
		
		<TextView
			android:id="@+id/account_info_help_moderate_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="4dp"
			android:layout_below="@+id/account_info_help_moderate_bar_one"
			android:layout_alignLeft="@+id/account_info_help_moderate_bar_one"
			android:text="@string/strength_bar_moderate"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		
		<TextView
			android:id="@+id/account_info_help_p2_title_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:layout_below="@+id/account_info_help_separator_line_one"
			android:paddingTop="8dp"
			android:text="@string/account_info_help_pass_strength_text"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		
		<TextView
			android:id="@+id/account_info_help_p2_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/account_info_help_p2_title_label"
			android:layout_marginTop="16dp"
			android:text="@string/account_info_help_pass_p2_text"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:textColor="@color/black"
			android:textSize="@dimen/light_text_size" />
		
		<View
			android:id="@+id/account_info_help_separator_line_two"
			android:layout_width="fill_parent"
			android:layout_height="1dp"
			android:layout_marginTop="16dp"
			android:layout_below="@+id/account_info_help_p2_label"
			android:background="@android:color/darker_gray" />
		
		<TextView
			android:id="@+id/account_info_help_p3_title_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:layout_below="@+id/account_info_help_separator_line_two"
			android:paddingTop="8dp"
			android:text="@string/account_info_help_pass_strength_text"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall" />
		
		<View
			android:id="@+id/account_info_help_strong_bar_one"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignLeft="@+id/account_info_two_uid_bar_one_help"
			android:layout_below="@+id/account_info_help_separator_line_two"
			android:layout_alignTop="@+id/account_info_help_p3_title_label"
			android:layout_marginTop="8dp"
			android:background="@color/green" />
		
		<View
			android:id="@+id/account_info_help_strong_bar_two"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_alignTop="@+id/account_info_help_strong_bar_one"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@+id/account_info_help_strong_bar_one"
			android:background="@color/green" />
		
		<View
			android:id="@+id/account_info_help_strong_bar_three"
			android:layout_alignTop="@+id/account_info_help_strong_bar_one"
			android:layout_width="32dp"
			android:layout_height="4dp"
			android:layout_marginLeft="8dp"
			android:layout_toRightOf="@+id/account_info_help_strong_bar_two"
			android:background="@color/green" />
	
		<TextView
			android:id="@+id/account_info_help_strong_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="4dp"
			android:layout_below="@+id/account_info_help_strong_bar_one"
			android:layout_alignLeft="@+id/account_info_help_strong_bar_one"
			android:text="@string/strength_bar_strong"
			android:textColor="@color/black"
			android:textAppearance="?android:attr/textAppearanceSmall"/>
		
		<TextView
			android:id="@+id/account_info_help_p3_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_below="@+id/account_info_help_p3_title_label"
			android:layout_marginTop="16dp"
			android:text="@string/account_info_help_pass_strong_text"
			android:textAppearance="?android:attr/textAppearanceSmall"
			android:textColor="@color/black"
			android:textSize="@dimen/light_text_size" />
		
		<TextView
			android:id="@+id/account_info_help_go_back_label"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="8dp"
			android:layout_below="@id/account_info_help_p3_label"
			android:clickable="true"
			android:onClick="goBack"
			android:padding="8dp"
			android:text="@string/go_back"
			android:textColor="@color/blue"/>
		
		<RelativeLayout
			android:layout_width="match_parent"
			android:layout_height="100dp"
			android:layout_below="@+id/account_info_help_go_back_label" >
			
			<include layout="@layout/old_general_need_help_footer"/>
			
			<include layout="@layout/old_general_copyright_footer"/>
			
		</RelativeLayout>

	</RelativeLayout>

</ScrollView>
