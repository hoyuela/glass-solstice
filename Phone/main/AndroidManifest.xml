<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.discover.mobile"
    android:versionCode="1"
    android:versionName="4.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="16" />
    
	<uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
	<uses-permission android:name="android.permission.READ_PHONE_STATE" />
	
    <!-- android:theme="@android:style/Theme.NoTitleBar" -->
    <application
    	android:name=".DiscoverApplication"
        android:icon="@drawable/discove_mobile_icn"
        android:label="@string/app_name"
        android:allowBackup="false" 
        android:theme="@style/AppTheme">
        
       	<activity 
            android:name=".login.StartActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoTitleBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" >
                </action>
                <category android:name="android.intent.category.LAUNCHER" >
                </category>
            </intent-filter>
       	</activity>
        
        
       <activity 
            android:name=".login.LoginActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoTitleBar">
       </activity>
       
       <activity 
            android:name="com.discover.mobile.register.EnhancedAccountSecurity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".login.LoggedInLandingPage"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".login.LockOutUserActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
        <activity 
            android:name="com.discover.mobile.register.AccountInformationActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
        <activity 
            android:name="com.discover.mobile.register.AccountInformationTwoActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
        <activity 
            android:name=".register.AccountInformationConfirmationActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".register.AccountInformationHelpActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".forgotuidpassword.ForgotCredentialsActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".forgotuidpassword.ForgotUserIdActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">
       </activity>
       
       <activity 
            android:name=".navigation.NavigationMenuRootActivity">
       </activity>

    </application>

</manifest>