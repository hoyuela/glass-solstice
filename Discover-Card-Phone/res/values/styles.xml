<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- fastcheck display -->
    <style name="OrangeTheme">
        <item name="android:background">@drawable/splash_screen</item>
    </style>
    <!--
    <style name="login_white_pane">
        <item name="android:layout_marginLeft">@dimen/forms_inner_padding</item>
        <item name="android:layout_marginRight">@dimen/forms_inner_padding</item>
        <item name="android:background">@drawable/login_white_panel</item>
    </style>
    -->
    <style name="login_bottom_bar_button">
        <item name="android:padding">@dimen/content_padding</item>
        <item name="android:textSize">@dimen/small_copy_min</item>
        <item name="android:background">@drawable/login_bottom_bar_button</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:height">@dimen/login_bottom_button_height</item>
    </style>

    <style name="forgot_credentials_title" parent="title_copy">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:textSize">@dimen/title_text_height</item>
        <item name="android:textStyle">bold</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="back_button_style">
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/title</item>
    </style>

    <style name="field_copy_fastcheck_timestamp" parent="discover_text">
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/field_copy</item>
    </style>

    <style name="title_copy_fastcheck_salutation" parent="discover_text">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- Confirmation Screen Styles -->
    <style name="confirmation_info_header_text">
        <item name="android:textColor">@color/field_copy</item>
        <item name="android:textSize">14dp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="confirmation_info_value_text">
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">20dp</item>
    </style>

    <!-- Style for strength level in help guide -->
    <style name="strength_help_level_message" parent="title_copy">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
    </style>

    <!-- Style for action message in help guide -->
    <style name="action_message" parent="small_copy">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/sub_copy</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="special_push_orange_button" parent="call_to_action_theme">
        <item name="android:textSize">@dimen/small_copy_mid</item>
    </style>

    <style name="push_toggle_box_default">
        <item name="android:padding">@dimen/between_related_elements_padding</item>
        <item name="android:layout_marginLeft">@dimen/between_related_elements_padding</item>
        <item name="android:background">@drawable/gray_gradient_square</item>
        <item name="android:src">@drawable/transparent_square</item>
    </style>

    <style name="push_toggle_box_text_style">
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="push_header_text_style">
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/body_copy_min</item>
    </style>

    <style name="push_toggle_item_secondary_text">
        <item name="android:textColor">@color/sub_copy</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_expand_item" parent="@style/blue_hyperlink">
        <item name="android:textSize">18sp</item>
    </style>

    <style name="push_header_item_text">
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="push_phone_number_text">
        <item name="android:textColor">@color/sub_copy</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="push_phone_number">
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="push_change_number">
        <item name="android:textColor">@color/blue_link</item>
        <item name="android:textSize">@dimen/small_copy_max</item>
    </style>

    <style name="push_manage_header_style">
        <item name="android:padding">@dimen/forms_inner_padding</item>
        <item name="android:background">@drawable/notification_list_item</item>
        <item name="android:layout_marginTop">@dimen/groups_of_elements_padding</item>
    </style>

    <style name="push_toggle_text">
        <item name="android:textColor">@color/body_copy</item>
        <item name="android:textSize">16sp</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>

    <style name="push_manage_save_success">
        <item name="android:textColor">@color/orange_link_idicator</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="discover_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="blue_hyperlink_forgot">
        <item name="android:textColor">@color/darkgray</item>
        <item name="android:textSize">@dimen/body_copy_min</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="blue_hyperlink_Privacy">
        <item name="android:textColor">@color/darkgray</item>
        <item name="android:textSize">@dimen/body_copy_min</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!--
    <style name="title_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">@dimen/title_min</item>
    </style>
    -->

    <style name="discover_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="body_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@color/darkgray</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="title_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
        <item name="android:textColor">@color/title</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="modal_title_text" parent="title_copy">
        <item name="android:layout_marginBottom">@dimen/modal_text_padding</item>
        <item name="android:layout_marginTop">@dimen/modal_text_padding</item>
        <item name="android:layout_marginBottom">@dimen/modal_text_padding</item>
        <item name="android:textSize">@dimen/title_min</item>
        <item name="android:textColor">@color/title</item>
    </style>

    <style name="modal_content_text" parent="discover_text">
        <item name="android:textColor">@color/sub_copy</item>
        <item name="android:textSize">@dimen/alert_txt</item>
        <item name="android:textStyle">normal</item>
        <item name="android:layout_marginBottom">@dimen/modal_alert</item>
    </style>

    <!-- NEED HELP? TEXT / #3d484d / 17ps font-size / centered / bold -->
    <style name="need_help_text" parent="body_copy">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textSize">@dimen/alert_footer</item>
        <!-- <item name="android:textStyle">bold</item> -->
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@color/darkgray</item>
    </style>

    <style name="modal_error_image">
        <item name="android:layout_marginRight">@dimen/forms_inner_padding</item>
        <item name="android:layout_marginTop">@dimen/image_top_padding</item>
    </style>
    <!-- <item name="android:layout_marginTop">@dimen/between_related_elements_padding</item> -->


    <!-- Blue Hyperlink styles -->


    <!-- 1-800-347-2683 NUMBER LINK / #0267b1/ #267bb1 / 17ps / regular -->
    <style name="contact_no_hyperlink">
        <item name="android:textColor">@color/footer_link</item>
        <item name="android:textSize">@dimen/alert_footer</item>
        <!-- <item name="android:textStyle">normal</item> -->
        <item name="android:textStyle">bold</item>
    </style>

    <style name="feedback_hyperlink">
        <item name="android:textColor">@color/footer_link</item>
        <item name="android:textSize">@dimen/alert_feedback</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="smallest_copy" parent="discover_text">
        <item name="android:textSize">@dimen/small_copy_mid</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/sub_copy</item>
    </style>

    <style name="small_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/sub_copy</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="orange_help_button">
        <item name="android:background">@drawable/transparent_square</item>
        <item name="android:src">@drawable/question_mark_icon</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
    </style>

    <style name="card_error_label" parent="discover_text">
        <item name="android:textColor">@color/error_indicator</item>
        <item name="android:visibility">gone</item>
        <item name="android:textSize">@dimen/error_min</item>
    </style>

    <style name="card_strong_auth_field_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceMedium</item>
        <item name="android:textColor">@color/body_copy</item>
    </style>

    <style name="card_strong_auth_field_copy_small" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/body_copy</item>
    </style>

    <style name="error_label" parent="discover_text">
        <item name="android:textColor">@color/error_indicator</item>
        <item name="android:visibility">gone</item>
    </style>

    <style name="call_to_action_theme">
        <item name="android:textColor">@drawable/call_to_action_text_color</item>
        <item name="android:textSize">@dimen/body_copy_max</item>
        <item name="android:paddingLeft">@dimen/orange_button_left_right_padding</item>
        <item name="android:paddingRight">@dimen/orange_button_left_right_padding</item>
        <item name="android:background">@drawable/call_to_action_button</item>
        <item name="android:paddingTop">@dimen/call_to_actions_padding</item>
        <item name="android:paddingBottom">@dimen/call_to_actions_padding</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="blue_hyperlink">
        <item name="android:textColor">@color/blue_link</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- FastView/QuickView -->
    <style name="quick_view_text">
        <item name="android:textColor">#3d484d</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="quick_view_faq">
        <item name="android:textColor">#267bb1</item>
        <item name="android:textStyle">bold</item>
    </style>
    <!-- FastView/QuickView -->


    <!-- Whats New -->

    <style name="whatsnew_small_copy" parent="discover_text">
        <item name="android:textAppearance">?android:attr/textAppearanceSmall</item>
        <item name="android:textColor">@color/whatsnew_desc</item>
        <item name="android:textStyle">normal</item>
    </style>
    <!-- Whats New -->


    <!-- /* 13.4  Changes Start* -->

    <style name="strongauth_ans_selection">
        <item name="android:textColorHighlight">#3d484d</item>
        <item name="android:textColor">#8a9499</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="spinner_style">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#3d484d</item>
        <item name="android:textSize">14sp</item>
        <item name="android:background">#ebeff0</item>
        <item name="android:hint">Select a Question</item>
        <item name="android:windowTitleStyle">@style/prompt_style</item>
    </style>

    <style name="prompt_style" parent="@android:style/Theme.Dialog">
        <item name="android:background">@android:color/black</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="HoloSpinner">
        <item name="android:background">@drawable/spinner_holo</item>
    </style>

    <style name="page_title">
        <item name="android:textColor">@color/title</item>
        <item name="android:textSize">@dimen/title_min</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- /* 13.4  Changes End* -->
    
     <!-- Passcode Styles start -->
    <style name="PasscodeDigit">
        <item name="android:textSize">@dimen/passcode_digit_text</item>
        <item name="android:typeface">serif</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">false</item>
        <item name="android:longClickable">false</item>
        <item name="android:layout_width">@dimen/passcode_digit_width</item>
        <item name="android:layout_height">@dimen/passcode_digit_height</item>
        <item name="android:layout_marginLeft">@dimen/passcode_digit_hpad</item>
        <item name="android:layout_marginRight">@dimen/passcode_digit_hpad</item>
        <item name="android:maxLength">1</item>
        <item name="android:digits">0123456789</item>
        <item name="android:keepScreenOn">true</item>
        <item name="android:inputType">textPassword</item>
        <item name="android:imeOptions">actionNone|flagNoExtractUi|flagNoAccessoryAction|flagNoEnterAction</item>
    </style>

    <style name="PasscodeGuidelines">
        <item name="android:textSize">@dimen/passcode_message_text</item>
        <item name="android:textColor">@color/passcodeBlue</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:text">@string/nomatch_pass</item>
    </style>

    <style name="ScreenRoot">
        <item name="android:background">@color/white</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="Spacer">
        <item name="android:layout_width">1.0px</item>
        <item name="android:layout_height">1.0px</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="PasscodeDigitParent">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">@dimen/passcode_vpad</item>
    </style>

    <style name="PasscodeOverlay">
        <item name="android:background">@color/white</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="PasscodeOverlayTitle">
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:textSize">25sp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/passcodeBlack</item>
    </style>

    <style name="PasscodeOverlayParagraph">
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/passcodeBlack</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="PasscodeHR">
        <item name="android:background">#9C9C9C</item>
        <item name="android:layout_marginTop">12dp</item>
        <item name="android:layout_marginBottom">12dp</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
    </style>
    
    <!-- Passcode Styles end -->

</resources>